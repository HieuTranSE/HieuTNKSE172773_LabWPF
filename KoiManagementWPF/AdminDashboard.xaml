<Window x:Class="KoiManagementWPF.AdminDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KoiManagementWPF"
        mc:Ignorable="d"
        Title="AdminDashboard" Height="450" Width="800">
    <Grid>
        <Label Content="Dashboard Koi Management System" HorizontalAlignment="Center" Height="53" Margin="0,51,0,0" VerticalAlignment="Top" Width="427" FontSize="24" FontWeight="Bold"/>
        <Button Content="Manage Accounts" HorizontalAlignment="Left" Height="48" Margin="159,179,0,0" VerticalAlignment="Top" Width="188" Click="ManageAccount_Click" FontWeight="Bold" Background="#FFC1E5D7" RenderTransformOrigin="1.162,-0.485" FontSize="15"/>
        <Button Content="Manage Advertisements" HorizontalAlignment="Center" Height="48" Margin="0,300,0,0" VerticalAlignment="Top" Width="188" Click="ManageAds_Click" FontWeight="Bold" Background="#FFDDE5C1" FontSize="15"/>
        <Button x:Name="ManageKoiFish" Content="Manage Fishes" HorizontalAlignment="Left" Height="48" Margin="458,179,0,0" VerticalAlignment="Top" Width="188" Click="ManageKoiFish_Click" FontWeight="Bold" Background="#FFC1C2E5" FontSize="15"/>

    </Grid>
</Window>
